{
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module",
        "ecmaFeatures": {
            "impliedStrict": true
        }
	},
	"env": {
		"browser": true,
		"node": true
	},
	"globals": {
		"Promise": true
	},
	"extends": "eslint:recommended",
    "rules": {
		"semi": "error",
		"no-unused-vars": "warn"
    }
}
